<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import jfxtras.scene.control.agenda.Agenda?>

<AnchorPane id="AnchorPane" prefHeight="500" prefWidth="800.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.CalendarController">
    <VBox alignment="CENTER" spacing="10.0" style="-fx-padding: 20px;">
    <HBox alignment="CENTER" spacing="10.0">
        <Button onAction="#handlePreviousYearButtonAction"  text="Previous Year" />
            <Button onAction="#handlePreviousMonthButtonAction"  text="Previous Month" styleClass="circle-button"/>
            <Button onAction="#handlePreviousWeekButtonAction"  text="Previous Week" />
            <Button onAction="#handleNextWeekButtonAction" text="Next Week" />
            <Button onAction="#handleNextMonthButtonAction"  text="Next Month" />
            <Button onAction="#handleNextYearButtonAction" text="Next Year" />
            
            
        </HBox>
        <Agenda fx:id="Calendar" maxHeight="500.0" maxWidth="800"/>
    </VBox>
    
</AnchorPane>
